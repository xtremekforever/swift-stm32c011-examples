{
  "cCompiler": {
    "extraCLIOptions": [
      "-mcpu=cortex-m0plus",
      "-mthumb",
      "-mfloat-abi=soft",
      "-ffreestanding",
      "-ffunction-sections",
      "-fshort-enums",
      "-gdwarf-4"
    ]
  },
  "swiftCompiler": {
    "extraCLIOptions": [
      "-Osize",
      "-Xfrontend", "-disable-stack-protector",
      "-Xfrontend", "-function-sections"
    ]
  },
  "schemaVersion": "1.0"
}