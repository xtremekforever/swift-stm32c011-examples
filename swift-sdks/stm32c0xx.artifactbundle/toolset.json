{
  "cCompiler": {
    "extraCLIOptions": [
      "-mcpu=cortex-m0plus",
      "-mthumb",
      "-mfloat-abi=soft",
      "-ffreestanding",
      "-ffunction-sections",
      "-fshort-enums"
    ]
  },
  "swiftCompiler": {
    "extraCLIOptions": [
      "-Osize",
      "-wmo",
      "-enable-experimental-feature", "Embedded",
      "-Xfrontend", "-disable-stack-protector",
      "-Xfrontend", "-function-sections"
    ]
  },
  "schemaVersion": "1.0"
}